#include "holberton.h"
#include <stdio.h>

/**
 * print_diagsums - prints the sum of two diagonals of a square matrix
 * @a: array
 * @size: size of diagonal
 * Return: void
 */
void print_diagsums(int *a, int size)
{
	int i;
	int sum1 = 0;
	int sum2 = 0;

	for (i = 0; i < size; i++)
	{
		sum1 += i * (size + 1);
		sum2 += (i + 1) * (size-1);
		printf("%d, %d\n", a[sum1, sum2], a[sum1, sum2]);
	}
}
